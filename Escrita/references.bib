@article{Calvete99,
title = "The bilevel linear/linear fractional programming problem ",
journal = "European Journal of Operational Research ",
volume = "114",
number = "1",
pages = "188 - 197",
year = "1999",
note = "",
issn = "0377-2217",
doi = "http://dx.doi.org/10.1016/S0377-2217(98)00078-2",
OPTurl = "http://www.sciencedirect.com/science/article/pii/S0377221798000782",
author = "Herminia I. Calvete and Carmen Gal\'e",
keywords = "Bilevel programming",
keywords = "Nonconvex optimization",
keywords = "Linear-fractional",
keywords = "Enumerative method ",
abstract = "Bilevel programming involves two optimization problems where the constraint region of the first level problem is implicitly determined by another optimization problem. In this paper we consider the bilevel linear/linear fractional programming problem in which the objective function of the first level is linear, the objective function of the second level is linear fractional and the feasible region is a polyhedron. For this problem we prove that an optimal solution can be found which is an extreme point of the polyhedron. Moreover, taking into account the relationship between feasible solutions to the problem and bases of the technological coefficient submatrix associated to variables of the second level, an enumerative algorithm is proposed that finds a global optimum to the problem. "
}

