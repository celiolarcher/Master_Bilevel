set grid
set xrange [0.95:18.1]
set yrange [-0.3:45] 
set title "Operações de Pivoteamento (Grupo Literatura)"
 set xlabel "Casos de Teste" 
 #set ylabel "" 
set xtics rotate
plot "LitrPivot.csv" using 0:9:10:8:xticlabels(1) t "Operações Pivo" with yerrorbars
replot "LitrPivot.csv" using 0:12:xticlabels(1) t "Restrições (Seguidor)" with lines 
set terminal png
set output 'PivotLemkeLit.png'
replot


set grid
set xrange [0.95:18.1]
set yrange [-0.3:70] 
set title "Operações de Pivoteamento (Grupo SMDquad)"
 set xlabel "Casos de Teste" 
set xtics rotate
plot "SMDPivot.csv" using 0:5:6:4:xticlabels(1) t "Operações Pivo" with yerrorbars
replot "SMDPivot.csv" using 0:8:xticlabels(1) t "Dimensão (Líder/Seguidor)" with lines 
set terminal png
set output 'PivotLemkeSMD.png'
replot



reset
set grid
set xrange [0.95:12.1]
set yrange [-0.3:95] 
set title "Operações de Pivoteamento (Grupo RESTRquad)"
 set xlabel "Casos de Teste" 
set xtics rotate
plot "RESTRPivot.csv" using 0:7:8:6:xticlabels(1) t "Operações Pivo" with yerrorbars
replot "RESTRPivot.csv" using 0:10:xticlabels(1) t "Dimensão (Líder/Seguidor)" with lines
