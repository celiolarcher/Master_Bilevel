set grid
set xrange [0.95:12.1]
set yrange [-0.3:75] 
set title "Operações de Pivoteamento (Grupo RESTRquad)"
 set xlabel "Casos de Teste" 
 #set ylabel "" 
#plot  "data" index 9 u 1:2 t "Best" with lines 
#plot  "data" every :1 u 1:2 t "Best" with lines 
#plot  "data" u 2 t "Best" with points
set xtics rotate
plot "RESTRPivot.csv" using 0:7:8:6:xticlabels(1) t "Operações Pivo" with yerrorbars
replot "RESTRPivot.csv" using 0:2:xticlabels(1) t "Dimensão (Líder/Seguidor)" with lines
replot "RESTRPivot.csv" using 0:3:xticlabels(1) t "Restrições (Líder)" with lines
replot "RESTRPivot.csv" using 0:5:xticlabels(1) t "Restrições (Seguidor)" with lines
set terminal png
set output 'RESTRPivot.png'
replot
