set grid
set title "Convergência ao ótimo"
 #set ylabel "" 
set xtics rotate

set key at 19.2,113
set style fill   solid 1.00 border lt -1

set xrange [0.5:18.8]
set yrange [-0.1:101] 

set style data histogram

plot 'DExLEMKESeguidorLIT.csv' using 4:xticlabels(1) ti "DE+LEMKE", 'DExLEMKESeguidorLIT.csv' using 6 ti "DE+DE"


set terminal png
set output 'DExLemkeAcertoLit.png'
replot


reset

set grid
set title "Avaliações do Líder"
 #set ylabel "" 
set xtics rotate

#set key at 19.2,113
set style fill   solid 1.00 border lt -1

set xrange [0.5:18.8]
set yrange [-0.1:5500] 

set style data histogram

plot 'DExLEMKESeguidorLIT.csv' using 5:xticlabels(1) ti "DE+LEMKE", 'DExLEMKESeguidorLIT.csv' using 7 ti "DE+DE"


set terminal png
set output 'DExLemkeAvalUPLit.png'
replot

reset

